class Solution {
    public int[][] transpose(int[][] matrix) {
      int result [][] = new int [matrix[0].length][matrix.length];  //[col[[row]]
      for ( int i = 0; i<matrix.length; i++){//row
        for (int j = 0; j<matrix[0].length; j++){//col
            result [j][i] = matrix [i][j];
        }
      }
      return result;
    }
}
